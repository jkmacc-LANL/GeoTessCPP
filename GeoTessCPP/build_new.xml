<?xml version="1.0"?>

<project name="GeoTessCPP" basedir="." default="rels">

  <target name="rels">
      <exec executable="gmake" />
      <exec executable="cp">
      	<arg value="lib/libgeotesscpp.so"/>
      	<arg value="../lib/lib_sun64/"/>
      </exec>
  </target>
  
  <target name="devl">
      <exec executable="gmake" />
      <exec executable="cp">
      	<arg value="lib/libgeotesscpp.so"/>
      	<arg value="../lib/lib_sun64/"/>
      </exec>
  </target>
  
  <target name="clean_all">
      <exec executable="gmake">
      <arg value="clean_objs"/>
      </exec>
  </target>
  	
</project>
